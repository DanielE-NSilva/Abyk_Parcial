<%- include('partials/_header') -%>

    <blockquote>
        <form class="login100-form">
            <span class="login100-form-title" style="padding-bottom: 0px;">
                Bienvenido Estudiante
            </span>
            <div id="estudiante" class="element">
                <br>
                <div class="wrap-input100">
                    <span class="symbol-input100">
                        <i class="fas fa-user-edit" aria-hidden="true"></i>
                    </span>
                    <div>
                        <input readonly="true" class="input100" type="text" name="nombre" id="Nombre"
                            placeholder="Ingrese Su Nombre" value="<%= data.NombreEstudiante %>">
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-6">
                        <div class="wrap-input100">
                            <span class="symbol-input100">
                                <i class="fas fa-user-edit" aria-hidden="true"></i>
                            </span>
                            <div>
                                <input readonly="true" class="input100" type="text" name="apellido1"
                                    placeholder="Ingrese Su Primer Apellido" value="<%= data.ApellidoUnoEstudiante %>">
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="wrap-input100">
                            <span class="symbol-input100">
                                <i class="fas fa-user-edit" aria-hidden="true"></i>
                            </span>
                            <div>
                                <input readonly="true" class="input100" type="text" name="apellido2"
                                    placeholder="Ingrese Su Segundo Apellido" value="<%= data.ApellidoDosEstudiante %>">
                            </div>
                        </div>
                    </div>
                </div>

                <div class="wrap-input100">
                    <span class="symbol-input100">
                        <i class="fas fa-at" aria-hidden="true"></i>
                    </span>

                    <div>
                        <input readonly="true" class="input100" type="text"
                            placeholder="Ingrese Su Correo Institucional" value="<%= data.EmailEstudiante %>">
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-7">
                        <div class="wrap-input100">
                            <span class="symbol-input100">
                                <i class="fas fa-id-card-alt" aria-hidden="true"></i>
                            </span>
                            <div>
                                <input readonly="true" class="input100" type="number" name="NroDocumento"
                                    placeholder="Ingrese Su Numero De IdentificaciÃ³n"
                                    value="<%= data.NroDocumentoEstudiante %>">
                            </div>
                        </div>
                    </div>
                    <div class="col-md-5">
                        <div class="wrap-input100">
                            <span class="symbol-input100">
                                <i class="fab fa-elementor" aria-hidden="true"></i>
                            </span>
                            <div>
                                <input readonly="true" class="input100" type="number" name="IDdocument" id="IDdocument"
                                    placeholder="Ingrese Su Plan Academico:" value="<%= data.PlanAcademico %>">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
        <div class="row" style="width: 380px;">
            <form class="col-md-6" action="/antecedentes" method="POST">

                <div style="display: none">
                    <input name="NroDocumento" value="<%= data.NroDocumentoEstudiante %>">
                    <input name="email" value="<%= data.EmailEstudiante %>">
                    <input name="TipoDeUsuario" value="estudiantes">
                    <input name="Encontrado" value="<%= data.encontrado%>" id="Encontrado">
                </div>

                <button class="login100-form-btn container-login100-form-btn" id="btn-Antecedentes">
                    <i class="fa fa-check-circle"></i> Antecentes
                </button>
            </form>
            <form class="col-md-6" action="/pasaporte" method="POST">
                <div style="display: none">
                    <input name="NroDocumento"  value="<%= data.NroDocumentoEstudiante %>">
                    <input name="email" value="<%= data.EmailEstudiante %>">
                    <input name="TipoDeUsuario"  value="estudiantes">
                    <input name="Encontrado" value="<%= data.encontrado%>" id="Encontrado">
                </div>
                <button class="login100-form-btn container-login100-form-btn" id="btn-encuesta">
                    <i class="fa fa-check-circle"></i> Pasaporte
                </button>
            </form>
        </div>
        <br>

        
        <form class="col-md-6" action="/infoFamiliar/<%= data.NroDocumentoEstudiante %>" method="GET">
            <button class="login100-form-btn container-login100-form-btn" id="btn-infofamilia">
                <i class="fa fa-check-circle"></i> Informacion familiar
            </button>
        </form>

        <script src="/public/js/Comprobacion.js"></script>

    </blockquote>

    <%- include('partials/_footer') -%>